{
  "name": "@flare-city/tsconfig",
  "version": "0.1.0",
  "description": "Common tsconfigs for different types of projects in FlareCity",
  "exports": {
    ".": {
      "default": "./tsconfig.worker.json",
      "node": "./tsconfig.worker.json",
      "import": "./tsconfig.worker.json",
      "require": "./tsconfig.worker.json"
    },
    "./worker": {
      "default": "./tsconfig.worker.json",
      "node": "./tsconfig.worker.json",
      "import": "./tsconfig.worker.json",
      "require": "./tsconfig.worker.json"
    },
    "./worker/build": {
      "default": "./tsconfig.worker-build.json",
      "node": "./tsconfig.worker-build.json",
      "import": "./tsconfig.worker-build.json",
      "require": "./tsconfig.worker-build.json"
    },
    "./library": {
      "default": "./tsconfig.library.json",
      "node": "./tsconfig.library.json",
      "import": "./tsconfig.library.json",
      "require": "./tsconfig.library.json"
    },
    "./library/build": {
      "default": "./tsconfig.library-build.json",
      "node": "./tsconfig.library-build.json",
      "import": "./tsconfig.library-build.json",
      "require": "./tsconfig.library-build.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/flare-city.git"
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/flare-city/issues"
  },
  "homepage": "https://github.com/drewdecarme/flare-city#readme",
  "dependencies": {
    "@cloudflare/workers-types": "4.20231025.0",
    "typescript": "5.2.2"
  }
}
